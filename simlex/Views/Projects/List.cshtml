@model SxExtantions.SxPagedCollection<simlex.ViewModels.VMProject>
@{
    ViewBag.Title = ViewBag.Title ?? "Проекты";
}

<h1 class="hidden">@(ViewBag.H1 ?? ViewBag.Title)</h1>

@if (Model.Collection.Any())
{
    for (int i = 0; i < Model.Collection.Length; i++)
    {
        @Html.DisplayFor(x => Model.Collection[i], new { Mode = "small" })
    }
}
else
{
    <div class="empty-page-panel">В данном разделе пока ничего не размещено, попробуйте зайти позднее...</div>
}
