@model SX.VkApi.Models.User[]
@if (Model.Any())
{
    SX.VkApi.Models.User item;
    <ul class="net-users">
        @for (int i = 0; i < Model.Length; i++)
        {
            item = Model[i];
            <li>
                <div class="net-users__item">
                    <a href="@($"http://vk.com/{item.Domain}")" target="_blank" rel="nofollow">
                        <img alt="@($"{item.FirstName} {item.LastName}")" src="@item.Photo_50" />
                    </a>
                </div>
            </li>
        }
    </ul>
}